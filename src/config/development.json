{
  "CUSTOM_TYPES": {
    "Address": "AccountId",
    "LookupSource": "AccountId",
    "AuctionId": "u64",
    "ClassId": "u64",
    "TokenId": "u64",
    "ClassData": "u32",
    "TokenData": {
      "locked": "bool"
    },
    "CID": "Vec<u8>",
    "ClassInfo": {
      "metadata": "Vec<u8>",
      "total_issuance": "TokenId",
      "owner": "AccountId",
      "data": "ClassData"
    },
    "TokenInfo": {
      "metadata": "Vec<u8>",
      "owner": "AccountId",
      "data": "TokenData"
    },
    "ClassInfoOf": "ClassInfo",
    "TokenInfoOf": "TokenInfo",
    "AuctionType": {
      "_enum": ["English", "Candle", "Dutch", "TopUp", "FixedSwap"]
    },
    "AuctionInfo": {
      "name": "Vec<u8>",
      "last_bid": "Option<(AccountId, Balance)>",
      "start": "BlockNumber",
      "end": "BlockNumber",
      "owner": "AccountId",
      "auction_type": "AuctionType",
      "token_id": "(u64, u64)",
      "minimal_bid": "Balance"
    },
    "AuctionInfoOf": "AuctionInfo"
  },
  "PROVIDER_SOCKET": "ws://127.0.0.1:9944",
  "RPC": {
    "auctions": {
      "auctions": {
        "description": "Get auctions",
        "params": [
          {
            "name": "AuctionId",
            "type": "AuctionId",
            "isOptional": true
          }
        ],
        "type": "AuctionInfoOf"
      }
    }
  }
}
